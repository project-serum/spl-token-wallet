    "web3": "^1.2.11"
  },
  "scripts": {
    "predeploy": "git pull --ff-only && yarn && yarn build",
    "deploy": "gh-pages -d build",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.js\" \"extension/src/*.js\" --write",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:extension": "yarn build && cp -a ./build/. ./extension/build/ && yarn build:extension-scripts",
    "build:extension-scripts": "cp -a ./extension/src/. ./extension/build/.",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "webextensions": true
    },
    "extends": [
      "react-app"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "^.+\\.cjs$"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git-format-staged -f 'prettier --stdin-filepath \"{}\"' src/*.js"
    }
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "git-format-staged": "^2.1.0",
    "husky": "^4.3.6",
    "prettier": "^2.2.1"
  }
}
